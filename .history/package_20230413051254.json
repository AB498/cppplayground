{
  "name": "cppplayground",
  "displayName": "C++ Playground - Live REPL",
  "publisher": "498",
  "description": "Inline reactive outputs on keyboard input",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "icon": "imgs/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cppplayground.enable",
        "title": "Start C++ Playground On All Files"
      },
      {
        "command": "cppplayground.disable",
        "title": "Stop C++ Playground On All Files"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AB498/cppplayground.git"
  },
  "homepage": "https://github.com/AB498/cppplayground/blob/main/README.md"
}
